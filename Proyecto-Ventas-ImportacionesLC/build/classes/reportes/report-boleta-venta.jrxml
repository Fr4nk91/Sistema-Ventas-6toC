	<parameter name="ID_VENTA" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     (detalle_ventas.`cantidad`*detalle_ventas.`precio_unitario`) AS precio_total,
  (SELECT SUM(dv.cantidad * dv.precio_unitario)
     FROM detalle_ventas dv
     WHERE dv.idventa = ventas.idventa) AS total_precio,
      CONCAT(clientes.`nombre`, ', ', clientes.`apellido`) AS cliente_nombre_completo,
     clientes.`direccion` AS clientes_direccion,
     clientes.`telefono` AS clientes_telefono,
     clientes.`documento` AS clientes_documento,
     ventas.`fecha` AS ventas_fecha,
     detalle_ventas.`cantidad` AS detalle_ventas_cantidad,
     detalle_ventas.`precio_unitario` AS detalle_ventas_precio_unitario,
       CONCAT(trabajadores.`nombre`, ', ', trabajadores.`apellido`) AS trabajador_nombre_completo,
     productos.`nombre` AS productos_nombre,
     productos.`tipoproducto` AS productos_tipoproducto,
     ventas.`idventa` AS ventas_idventa,
     usuarios.`usuario` AS usuarios_usuario
FROM
     `clientes` clientes INNER JOIN `ventas` ventas ON clientes.`idcliente` = ventas.`idcliente`
     INNER JOIN `detalle_ventas` detalle_ventas ON ventas.`idventa` = detalle_ventas.`idventa`
     INNER JOIN `usuarios` usuarios ON ventas.`idusuario` = usuarios.`idusuario`
     INNER JOIN `trabajadores` trabajadores ON usuarios.`idtrabajador` = trabajadores.`idtrabajador`
     INNER JOIN `productos` productos ON detalle_ventas.`idproducto` = productos.`idproducto`
WHERE ventas.`idventa` = $P{ID_VENTA}
GROUP BY
     clientes.`nombre`,
     clientes.`apellido`,
     clientes.`direccion`,
     clientes.`telefono`,
     clientes.`documento`,
     ventas.`fecha`,
     detalle_ventas.`cantidad`,
     detalle_ventas.`precio_unitario`,
     trabajadores.`nombre`,
     trabajadores.`apellido`,
     productos.`nombre`,
     productos.`tipoproducto`,
     ventas.`idventa`,
     usuarios.`usuario`
ORDER BY
     ventas.`fecha` ASC]]>
	</queryString>
